<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600" prefWidth="800" styleClass="border-box" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.projetointegrador.controllers.TelaClienteController">
   <center>

   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" styleClass="top" stylesheets="@../css/style.css" BorderPane.alignment="CENTER">
         <children>
            <Label styleClass="title" stylesheets="@../css/style.css" text="Lidos" />
         </children></HBox>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" styleClass="border-box-center" stylesheets="@../css/style.css" BorderPane.alignment="CENTER">
         <children>
            <Pane prefHeight="79.0" prefWidth="200.0" />
            <VBox maxWidth="-Infinity" prefHeight="200.0" prefWidth="670.0" styleClass="cadCliente-container" stylesheets="@../css/style.css">
               <children>
                  <Label styleClass="email-senha" stylesheets="@../css/style.css" text="Nome" />
                  <TextField fx:id="tfNome" styleClass="email-senha-textfild" stylesheets="@../css/style.css" />
                  <Label styleClass="email-senha" stylesheets="@../css/style.css" text="Email" />
                  <TextField fx:id="tfEmail" styleClass="email-senha-textfild" stylesheets="@../css/style.css" />
                  <Label styleClass="email-senha" stylesheets="@../css/style.css" text="Senha" />
                  <TextField fx:id="tfSenha" styleClass="email-senha-textfild" stylesheets="@../css/style.css" />
               </children>
            </VBox>
            <HBox maxHeight="-Infinity" prefHeight="100.0" prefWidth="200.0" styleClass="entrar-container" stylesheets="@../css/style.css">
               <children>
                  <Button maxHeight="-Infinity" mnemonicParsing="false" onAction="#voltar" prefHeight="6.0" styleClass="entrar-button" stylesheets="@../css/style.css" text="Cancelar" />
                  <Button maxHeight="-Infinity" mnemonicParsing="false" onAction="#cadastrar" prefHeight="6.0" styleClass="entrar-button" stylesheets="@../css/style.css" text="Cadastrar" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>

</BorderPane>
